{
  "name": "sup-react",
  "displayName": "Supercharge React",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "author": {
    "name": "Boris Yordanov",
    "email": "boris.yordanov11@yahoo.com"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:sup-react.newComponent",
    "onCommand:sup-react.newPage",
    "onCommand:sup-react.newModule",
    "onCommand:sup-react.sup"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sup-react.newComponent",
        "title": "new component"
      },
      {
        "command": "sup-react.newPage",
        "title": "new page"
      },
      {
        "command": "sup-react.newModule",
        "title": "new module"
      },
      {
        "command": "sup-react.sup",
        "title": "Say wassup"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
